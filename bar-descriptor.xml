<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>com.example.AudioPlayer</id>
    <name>AudioPlayer</name>
    <versionNumber>1.0.0</versionNumber>
    <description>DESCRIPTION</description>
    <author>Eugen</author>
    <copyright>COPYRIGHT</copyright>
    <initialWindow>
        <autoOrients>true</autoOrients>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
    </initialWindow>
    <env value="app/native/imports:/accounts/devuser/qt5/qml" var="QML2_IMPORT_PATH"/>
    <env value="app/native/plugins:app/native/imports:/accounts/devuser/qt5/plugins" var="QT_PLUGIN_PATH"/>
    <env value="app/native/lib:app/native/imports:/accounts/devuser/qt5/lib" var="LD_LIBRARY_PATH"/>
    <env value="1" var="QV4_FORCE_INTERPRETER"/>
    <arg>-platform</arg>
    <arg>qnx</arg>
    <action system="true">run_native</action>
    <action>access_shared</action>
    <asset path="AudioPlayer" type="Qnx/Elf" entry="true">AudioPlayer</asset>
    <asset path="%SRC_DIR%/qml">qml</asset>
    <!--
         %QT_INSTALL_LIBS%, %QT_INSTALL_PLUGINS, %QT_INSTALL_QML% and %SRC_DIR%
         tags are replaced on-the-fly when deploying the app. in Qt Creator.
    -->
    <!--
         By default this bar-descriptor.xml embeds Qt5 runtime
         into the application itself. See the following document
         for various deployments of Qt runtime to a device:
         http://qt-project.org/wiki/Qt-on-BlackBerry-devices
    -->
    <authorId>gYAAgIXh7nKLiw3suMscYm1lbdk</authorId>
</qnx>
